<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Gift List Sequence: BBP-YYMM-XXXX -->
        <record id="seq_gift_list" model="ir.sequence">
            <field name="name">Gift List Sequence</field>
            <field name="code">baby.list</field>
            <field name="prefix">BBP-%(year)s%(month)s-</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
    </data>
</odoo>
